{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Elastic.Serilog.Sinks",
      "Serilog.Sinks.Async"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": {
      "Async": {
        "Name": "Async",
        "Args": {
          "Configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "[{TimeStamp:yyyy-MM-dd HH:mm:ss.fff} zzz {Level:u3}] {Message:lj}{NewLine}{Exception}{StackTrace}",
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
              }
            },
            {
              "Name": "Elasticsearch",
              "Args": {
                "nodeUris": "http://localhost:9200",
                "indexFormat": "logstash-{0:yyyy.MM.dd}",
                "autoRegisterTemplate": true,
                "inlineFields": true,
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}{StackTrace}",
                "restrictedToMinimumLevel": "Information"
              }
            }
          ]
        }
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Discord": {
    "Prefix": "!",
    "AllowMention": true,
    "Token": "",
    "ChannelId": ""
  },
  "Github": {
    "User": "Nguyenhoangdung335",
    "RepositoryName": "Test.DiscordApp",
    "DefaultBranch": "main"
  },
  "ConnectionStrings": {
    "Github": "https://api.github.com"
  }
}
